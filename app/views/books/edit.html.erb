<h1>Editing Book</h1>

<%= form_for(@book) do |f| %>
	<div>
		<div>Title</div>
		<div><%= f.text_field :title %></div>
	</div>

	<div>
		<div>Body</div>
		<div><%= f.text_area :body %></div>
	</div>

	<%= f.submit 'Update Book' %>

<% end %>

<%= link_to "Show", book_path(@book) %>
<%= link_to "Back", books_path %>